[project]
	description = 
[access "refs/*"]
  read = group centos-opstools/opstools-ansible-core
  owner = group centos-opstools/opstools-ansible-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group centos-opstools/opstools-ansible-core
	label-Code-Review = -2..+2 group centos-opstools/opstools-ansible-ptl
	label-Verified = -2..+2 group centos-opstools/opstools-ansible-ptl
	label-Workflow = -1..+1 group centos-opstools/opstools-ansible-core
	label-Workflow = -1..+1 group centos-opstools/opstools-ansible-ptl
	label-Workflow = -1..+0 group Registered Users
	submit = group centos-opstools/opstools-ansible-ptl
	read = group centos-opstools/opstools-ansible-core
    read = group Registered Users
[access "refs/meta/config"]
    read = group centos-opstools/opstools-ansible-core
    read = group Registered Users
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
