 {
   "sensu": {{ sensu_datacenters|to_json }},
   "uchiwa": {
     "host": "{{ uchiwa_bind }}",
     "port": {{ uchiwa_port }},
     "refresh": {{ uchiwa_refresh }},
     "users": [
       {% for user in uchiwa_credentials -%}
       {
         "username": "{{ user.username }}"
         "password": "{{ user.password }}",
         "role": {
           "readonly": {{ user.readonly }}
         }
       }{% if not loop.last -%},{% endif %}
       {% endfor %}
     ]
   }
}
